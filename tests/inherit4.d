class Animal {
  void Method1() {
    Print("Animal");
  }
}

class Cow extends Animal {
   void Method1() {
    Print("Cow");
  }
}
class Jersey extends Cow {
   void Method1() {
      Print("Jersey");
   }
}

int main() {
  Animal a;

   a = new Animal;
   a.Method1();
  a = new Cow;
  a.Method1();
  a = new Jersey;
  a.Method1();
}
